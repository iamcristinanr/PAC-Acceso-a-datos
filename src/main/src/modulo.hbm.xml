<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
     <!--  MAPEO CLASE MODULO - TABLA MODULO - BBDD M06 -->
    	<class name="pac.Modulo"  table="modulos" catalog="m06">
    	<!--  GENERADOR AUTOINCREMENTAL ID -->
    		<id name="id">
    			<column name="idModulo"/>
    			<generator class="increment"/>
    		</id>
    		<!--  MAPEO PROPIEDADES CLASE MODULO -->
    		<property name="nombre" type="string"></property>
    		<property name="codigo" type="string"></property>
    	</class>
    
    </hibernate-mapping>